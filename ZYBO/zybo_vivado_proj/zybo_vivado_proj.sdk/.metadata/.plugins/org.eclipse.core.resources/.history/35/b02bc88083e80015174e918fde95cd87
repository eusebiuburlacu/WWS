#ifndef SCREENINTERFACE_H_
#define SCREENINTERFACE_H_

class IScreen
{
	IScreen();
	virtual ~IScreen();

	void sendIncCmd() = 0;
	void sendDecrCmd() = 0;
	void sendChScrCmd() = 0;
	void sendConfirmCmd() = 0;
	void printData() = 0;
};

#endif /* SCREENINTERFACE_H_ */
